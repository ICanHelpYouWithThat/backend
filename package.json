    "helmet": "^3.1.0",
    "http": "0.0.0",
    "jsonwebtoken": "^7.1.9",
    "mocha": "^3.1.2",
    "mysql": "^2.12.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.11.0",
    "piping": "^1.0.0-rc.4",
    "sequelize": "^3.25.0",
    "traceur": "0.0.111"
  },
  "devDependencies": {
